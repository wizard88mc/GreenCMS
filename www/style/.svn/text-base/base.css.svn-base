

  
html {
    color: #000;
    background: #30343B url('../img/css1-bkg.gif') repeat-x top left;
}

body, div, dl, dt, dd, ul, ol, li, h1, h2, h3, h4, h5, h6, pre, code, form, fieldset, legend, input, textarea, p, blockquote, th, td, address {
    margin: 0;
    padding: 0;
}

fieldset, img {
    border: 0;
}

address, caption, code, dfn, em, strong, th, var {
    font-style: normal;
    font-weight: normal;
}

li {
    list-style: none;
}

caption, th {
    text-align: left;
}

h1, h2, h3, h4, h5, h6 {
    font-size: 100%;
    font-weight: normal;
}

q:before, q:after {
    content: '';
}

abbr, acronym {
    border: 0;
    font-variant: normal;
}

sup {
    vertical-align: text-top;
}

sub {
    vertical-align: text-bottom;
}

input, textarea, select {
    font-family: inherit;
    font-size: inherit;
    font-weight: inherit;
}

legend {
    color: #000;
}

body {
    font-family: sans-serif;
}

pre, code, kbd, samp, tt {
    font-family: monospace;
    line-height: 100%;
}

body {
    font-family:"Droid Sans", Verdana, "Helvetica Neue", Helvetica, Arial, Geneva, sans-serif;
    color: #333;
    font-size: 80%;
}

em {
    font-style: italic;
}

strong {
    font-weight: bold;
}

a:link, a:visited, a:hover {
    text-decoration: none;
}

#header {
    margin-right: 2%;
}

#quickMenu {
    display: none;
}

#contents-right a:visited, #footer a:visited, #layout a:visited {
    border-bottom-width: 2px;
    border-bottom-style: solid;
    border-bottom-color: #8e3000;
    color: #8e3000;
}

#contents-right a, #header a, #footer a, #layout a, #accessibility a:visited, #layout a:visited, #path a:visited, #webmaster a, #webmaster a:visited {
    border-bottom-width: 2px;
    border-bottom-style: solid;
    border-bottom-color: #A8C0CF;
    color: #003366;
}

#contents a:hover, #header a:hover, #footer a:hover, #webmaster a:hover {
    border-bottom-width: 2px;
    border-bottom-style: solid;
    border-bottom-color: #003366;
    color: #003366;
}

#headImg {
    width: 19%;
    padding: 0.4em 1% 0.4em 0;
    float: left;
    background: #30343B url('../img/css1-bkg.gif') repeat-x top left;
}

#headerUtility {
    text-align: right;
    float: right;
    width: 40%;
    font-size: 0.8em;
    padding-right: 0.8em;
}

#headerUtility form {
    padding-top: 0.8em;
}

#textInput {
    width: 40%;
}

#headerText {
    padding-left: 1.5em;
    z-index: 1;
    margin-left: 20%;
    background-color: #EEEEEE;
    color: #30343b;
    padding-bottom: 1em;
}

#whiteSpace {
    padding-bottom: 0px;
}

#main {
    clear: both;
    background-color: #EEEEEE;
    margin: 0 2% 0 0;
}

#navigation {
    background-color: #EEEEEE;
    padding-top: 0.8em;
    float: left;
    width: 20%;
    color: #003366;
}

#navigation a {
    color: #003366;
}

#navigation a:visited, #secondLevel a:visited {
    color: #8e3000;
}

#navigation li {
    margin: 0 7% 0 7%;
    border-bottom-width: 1px;
    border-bottom-style: solid;
    border-bottom-color: #DDDDDD;
    line-height: 1.5;
}

#secondLevelSelected {
    font-weight: bold;
    color: #003366;
    background-color: white;
    padding: 0.4em 1% 0.5em 1%;
}

#navigation a {
    text-decoration: none;
    padding: 0.3em 7% 0.3em 7%;
    display: block;
    padding-top: 5px;
    padding-right: 0pt;
    padding-bottom: 5px;
    padding-left: 5px;
}

#navigation-current {
    padding-top: 5px;
    padding-bottom: 5px;
    padding-left: 5px;
    font-weight: bolder;
    background-color: white;
}

#secondLevel {
    margin-bottom:1.5em;
    line-height: 2em;
    background-color: #F7F7F7;
}

#secondLevel li {
    display: inline;
    border: none;
}

#secondLevel a, #secondLevel a:visited {
    padding: 0.4em 1% 0.5em 1%;
    border-bottom-width: 0;
    border-style: none;
}

#navigation a:hover, #secondLevel a:hover {
    border: none;
    background-color: #fafafa;
    color: #003366;
}

#navigation h3 {
    line-height: 1.5;
    padding: 1em;
    font-variant: small-caps;
    font-weight: bold;
    font-size: larger;
}

#utilities {
    float: right;
    width: 25%;
    padding: 1.5em 0 0.8em 0;
    background-color: white;
}

#headerUtility {
    line-height: 1.5em;
}

#contents-right {
    background-color: white;
    margin-left: 20%;
}

#contentsLong {
    background-color: white;
    padding: 1.5em;
    padding-right: 2%;
    padding-left: 2em;
}

#contents {
    background-color: white;
    padding: 1.5em;
    padding-right: 1%;
    padding-left: 2em;
    margin-right: 26%;
}

.column {
    margin-right: 26%;
}

#contets caption, #contentsLong caption{
  display:none;
}
#contents table, #contentsLong table  {
    border: none;
    margin: 0 auto 0 auto;
}

#contents td, #contentsLong td {
    border: 1px solid #BBB;
    padding: 0.5em 0.5em 0.5em 0.5em;
}

#contents th, #contentsLong th {
    border: 1px solid #BBB;
    background-color: #30343B;
    color: white;
    font-weight: bolder;
    padding: 0.5em 0.5em 0.5em 0.5em;
}

#contents h2, #contentsLong h2 {
    color: #30343B;
    font-variant: small-caps;
    font-size: 1.5em;
    margin-top: 0.4em;
    background-color: #eeeeee;
    font-size: 2em;
    padding-left: 0.8em;
}

#contents h3, #contentsLong h3 {
    color: #30343B;
    font-variant: small-caps;
    font-size: 1.5em;
    margin-top: 0.4em;
    font-size: 2em;
    padding-left: 0.8em;
}

#contents dl , #contentsLong dl {
    padding-top: 1em;
    padding-bottom: 1em;
}

#contents dt , #contentsLong dt{
    background-color: #30343B;
    color: white;
    line-height: 3em;
    font-size: large;
    padding-left: 1em;
    font-variant: small-caps;
    font-weight: bolder;
}

#contents dd , #contentsLong dd{
    padding-left: 2em;
    line-height: 3em;
    background-color: #EEEEEE;
    border-bottom-width: 1px;
    border-bottom-style: solid;
    border-bottom-color: #F7F7F7;
}

#footer {
    clear: both;
    text-align: center;
    background: #EEEEEE url('../img/footer.gif') repeat-x top left;
    margin-right: 2%;
    padding: 0.8em 2em 3em 2em;
}

#header h1 {
    font-size: 2em;
}

#selezionato {
    text-decoration: none;
    padding: 0.3em 72% 0.3em 7%;
    display: block;
    padding-top: 5px;
    padding-right: 0pt;
    padding-bottom: 5px;
    padding-left: 5px;
}

#header h1 a {
    border-style: none;
}

#utilities p {
    padding-left: 0.8em;
    margin-bottom: 0.8em;
    background-color: #eeeeee;
    color: #30343B;
}

#utilities h3 {
    font-variant: small-caps;
    font-size: large;
    background-color: #30343B;
    color: white;
    line-height: 2em;
    font-size: large;
    padding-left: 1em;
    font-weight: bolder;
}

#utilities dt {
    background-color: #F7F7F7;
    color: #30343B;
    line-height: 2em;
    font-size: large;
    padding-left: 1em;
    font-variant: small-caps;
    font-weight: bolder;
}

#utilities dd {
    padding: 0 0.8em 0 0.8em;
    background-color: #EEEEEE;
}

#utilities dt a {
}

.newsDate {
    font-size: 0.7em;
}

#contents img , #contentsLong img {
    float: right;
    width: 30%;
    padding-left: 0.8em;
}

#pathSelected {
    padding-bottom: 0.8em;
    background: white url('../img/path-selected.gif') no-repeat center bottom;
}

#webmaster {
    padding-right: 0.8em;
    padding-top: 0.8em;
    text-align: right;
    background-color: #EEEEEE;
    clear: both;
}